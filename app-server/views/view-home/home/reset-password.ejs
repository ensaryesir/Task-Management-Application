    <!-- Reset Password  -->
    <br /><br /><br />
    <section id="resetPassword">
      <br /><br />
      <section class="vh-100">
        <div class="container py-5 h-100">
          <div
            class="row d-flex align-items-center justify-content-center h-100"
          >
            <div class="col-md-8 col-lg-7 col-xl-6">
              <img
                src="https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-login-form/draw2.svg"
                class="img-fluid iq-mt-60"
                alt="Phone image"
              />
            </div>
            <div class="col-md-7 col-lg-5 col-xl-5 offset-xl-1">
              <form
                action="/reset-password"
                method="POST"
                onsubmit="return validatePassword()"
              >
                <img
                  src="../public-home/images/color-customizer/color-1.png"
                  class="img-fluid mb-4"
                  alt="Phone image"
                />
                <!-- Email input -->
                <div class="form-outline mb-4">
                  <input
                    type="email"
                    id="readOnlyInput"
                    class="form-control form-control-lg"
                    name="email"
                    value="<%= resetEmail %>"
                    readonly
                  />
                  <label class="form-label" for="readOnlyInput">Email</label>
                </div>
                <!-- Password input -->
                <div class="form-outline mb-4">
                  <input
                    type="password"
                    id="form1Example13"
                    class="form-control form-control-lg"
                    name="password"
                    required
                  />
                  <label class="form-label" for="form1Example13"
                    >Password</label
                  >
                </div>

                <!-- Password Confirm input -->
                <div class="form-outline mb-4">
                  <input
                    type="password"
                    id="form1Example23"
                    class="form-control form-control-lg"
                    name="confirmPassword"
                    required
                  />
                  <label class="form-label" for="form1Example23"
                    >Confirm Password</label
                  >
                </div>

                <div
                  class="d-flex justify-content-around align-items-center mb-4"
                >
                  <!-- Checkbox -->
                  <a href="/login" style="color: #5773ff">Login</a>
                  <a href="/registration" style="color: #5773ff"
                    >Registration</a
                  >
                </div>

                <!-- Submit button -->
                <button
                  type="submit"
                  class="btn btn-primary btn-lg btn-block"
                  style="background-color: #5773ff; font-size: medium"
                >
                  Change Password
                </button>

                <div class="divider d-flex align-items-center my-4">
                  <div class="container">
                    <div class="row justify-content-center">
                      <div class="col-auto">
                        <hr class="flex-grow-1" />
                      </div>
                      <div class="col-auto">
                        <p class="text-center fw-bold mx-3 mb-0 text-muted">
                          OR
                        </p>
                      </div>
                      <div class="col-auto">
                        <hr class="flex-grow-1" />
                      </div>
                    </div>
                  </div>
                </div>
                <div class="d-flex justify-content-center">
                  <div class="col-lg-10 col-md-6 col-sm-6">
                    <ul class="info-share d-flex justify-content-between">
                      <li>
                        <a href="#"><i class="fa fa-google"></i></a>
                      </li>
                      <li>
                        <a href="#" target="_blank"
                          ><i class="fa fa-linkedin"></i
                        ></a>
                      </li>
                      <li>
                        <a href="#" target="_blank"
                          ><i class="fa fa-github"></i
                        ></a>
                      </li>
                      <li>
                        <a href="#" target="_blank"
                          ><i class="fa fa-instagram"></i
                        ></a>
                      </li>
                      <li>
                        <a href="#" target="_blank"
                          ><i class="fa fa-twitter"></i
                        ></a>
                      </li>
                    </ul>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </section>
    </section>
