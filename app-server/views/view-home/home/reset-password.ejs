    <script>
      function validatePassword() {
        var password = document.getElementById("form1Example13").value;
        var confirmPassword = document.getElementById("form1Example23").value;

        if (password !== confirmPassword) {
          alert(
            "Passwords do not match. Please make sure the passwords match."
          );
          return false; // Formun gönderilmesini engellemek için false döndürün
        }

        return true; // Formun gönderilmesine izin vermek için true döndürün
      }
